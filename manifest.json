      },
      "server": {
        "title": "Server",
        "type": "string",
        "default": "backoffice.oct8ne.com/",
        "enum": [
          "backoffice-eu.oct8ne.com",
          "backoffice.oct8ne.com/"
        ],
        "enumNames": [
          "EU",
          "USA"
        ]
      }
    }
  },
