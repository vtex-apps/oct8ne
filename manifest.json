{
  "vendor": "vtex",
  "name": "oct8ne",
  "version": "1.0.5",
  "title": "Oct8ne",
  "description": "Oct8ne is a chat solution",
  "builders": {
    "pixel": "0.x",
    "docs": "0.x"
