{
  "vendor": "vtex",
  "name": "oct8ne",
  "version": "3.3.1",
  "title": "Oct8ne",
  "description": "Oct8ne is a chat solution",
  "builders": {
    "pixel": "0.x",
    "docs": "0.x",
    "react": "3.x",
    "store": "0.x"
  },
  "billingOptions": {
    "support": {
      "url": "https://oct8ne.com/terms-of-service/"
    },
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "settingsSchema": {
    "title": "Oct8ne",
    "type": "object",
    "properties": {
      "licenseId": {
        "title": "Oct8ne License",
        "description": "Enter the Oct8ne License",
        "type": "string"
      },
      "baseUrl": {
        "title": "Base URL",
        "description": "Enter the base URL",
        "type": "string"
      },
      "server": {
        "title": "Server",
        "type": "string",
        "default": "backoffice.oct8ne.com/",
        "enum": [
          "backoffice-eu.oct8ne.com/",
          "backoffice.oct8ne.com/"
        ],
        "enumNames": [
          "EU",
          "USA"
        ]
      },
      "oct8neAppUrl": {
        "title": "Oct8ne app url",
        "description": "Enter the url to read the oct8ne app script from",
        "default": "https://ecommerceproxy-cdn.oct8ne.com/scripts/vtexio/oct8neApp.js",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
